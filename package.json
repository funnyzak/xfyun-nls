{
  "name": "@funnyzak/xfyun-tts",
  "version": "0.9.0",
  "description": "a xfyun tts node module.",
  "main": "index.js",
  "files": [
    "lib/**/*",
    "index.js"
  ],
  "types": "lib/tts.d.ts",
  "scripts": {
    "lint": "eslint --fix src",
    "test": "npm run build && mocha test/*.test.js",
    "dev": "watch \"npm run build\" src",
    "build": "babel src -d lib",
    "prepublish": "npm run build",
    "release": "np",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/funnyzak/xfyun-tts.git"
  },
  "keywords": [
    "xfyun tts",
    "tts",
    "xfyun",
    "nodejs",
    "funnyzak"
  ],
  "author": "eric <silenceace@gmail.com>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/funnyzak/xfyun-tts/issues"
  },
  "homepage": "https://github.com/funnyzak/xfyun-tts#readme",
  "dependencies": {
    "chalk": "4.1.2",
    "crypto-js": "4.1.1",
    "ws": "8.2.3"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "core-js": "3.19.1",
    "eslint": "8.2.0",
    "expect.js": "0.3.1",
    "husky": "7.0.4",
    "lint-staged": "12.0.3",
    "mocha": "9.1.3",
    "muk": "0.5.3",
    "np": "*",
    "prettier": "2.4.1",
    "typescript": "4.5.2",
    "watch": "1.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3.3.5"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "np": {
    "yarn": false,
    "contents": ".",
    "branch": "master",
    "releaseDraft": true
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
